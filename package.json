{
  "name": "@martinhipp/rrule",
  "version": "1.0.5",
  "description": "RFC 5545 compliant recurrence rule library for parsing and generating iCalendar RRULE patterns",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "rrule",
    "recurrence",
    "icalendar",
    "rfc5545",
    "typescript",
    "calendar",
    "date",
    "datetime",
    "recurring",
    "schedule"
  ],
  "author": "Martin Hipp <martin.hipp@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinhipp/rrule.git"
  },
  "homepage": "https://github.com/martinhipp/rrule",
  "bugs": {
    "url": "https://github.com/martinhipp/rrule/issues"
  },
  "sideEffects": false,
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^20.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0"
  }
}
